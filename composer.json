{
    "name": "sotnaspina/laravel-model-caching",
    "description": "Automatic caching for Eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "fico7489/laravel-pivot": "*",
        "illuminate/cache": "5.3 - 5.5",
        "illuminate/config": "5.3 - 5.5",
        "illuminate/console": "5.3 - 5.5",
        "illuminate/database": "5.3 - 5.5",
        "illuminate/support": "5.3 - 5.5",
        "php": ">=5.6"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "orchestra/database": "3.6.x-dev@dev",
        "orchestra/testbench": "^3.6",
        "orchestra/testbench-browser-kit": "^3.6",
        "php-coveralls/php-coveralls" : "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "*",
        "sebastian/phpcpd": "*",
        "symfony/thanks": "^1.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/factories"
        ],
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelModelCaching\\Providers\\Service"
            ]
        }
    }
}
